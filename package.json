{
  "name": "@ranchhandrobotics/babylon_ros",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ranch-Hand-Robotics/babylon_ros.git"
  },
  "description": "Babylon.JS abstractions for ROS 2",
  "version": "0.1.10",
  "main": "dist/ros.js",
  "module": "dist/ros.js",
  "types": "types/src/ros.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ros.js",
      "types": "./types/src/ros.d.ts"
    }
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "test": "jest --ci --coverage --reporters=default --reporters=jest-junit",
    "build": "webpack --mode development",
    "build-verbose": "webpack --mode development --stats verbose",
    "watch": "webpack --mode development --watch",
    "pretest": "npm run build",
    "prepublishOnly": "webpack --mode=production",
    "dev-server": "npx http-server -c-1 --cors -p 8080 ."
  },
  "keywords": [
    "babylonjs",
    "ros",
    "ros2"
  ],
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "jest-junit": {
    "outputDirectory": "reports",
    "outputName": "jest-junit.xml",
    "ancestorSeparator": " â€º ",
    "uniqueOutputName": "false",
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  },
  "author": "Ranch Hand Robotics, LLC",
  "license": "MIT",
  "dependencies": {
    "@polyhobbyist/babylon-collada-loader": "^0.0.8",
    "@types/xml2js": "^0.4.14",
    "@types/xmldom": "^0.1.34",
    "babylonjs": "8.1.x",
    "babylonjs-gui": "8.1.x",
    "babylonjs-loaders": "8.1.x",
    "babylonjs-materials": "8.1.x",
    "babylonjs-viewer": "7.42.x",
    "buffer": "^6.0.3",
    "jest-environment-jsdom": "^29.7.0",
    "stream-browserify": "^3.0.0",
    "string_decoder": "^1.3.0",
    "timers-browserify": "^2.0.12",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "source-map-loader": "^5.0.0",
    "string-replace-loader": "^3.1.0",
    "ts-jest": "^29.3.1",
    "ts-loader": "^9.5.2",
    "tslib": "^2.8.1",
    "typescript": "~5.7.0",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  }
}
